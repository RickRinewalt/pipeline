{
  "name": "yolo-pro-pipeline",
  "version": "2.0.0",
  "description": "YOLO-PRO: Complete automation workflows for development",
  "main": "yolo-pro/src/cli/index.js",
  "bin": {
    "yolo-pro": "yolo-pro/src/cli/index.js"
  },
  "scripts": {
    "start": "node yolo-pro/src/cli/index.js",
    "test": "jest --testTimeout=30000",
    "test:coverage": "jest --coverage --testTimeout=30000",
    "test:watch": "jest --watch --testTimeout=30000",
    "lint": "eslint yolo-pro/src/**/*.js tests/**/*.js",
    "lint:fix": "eslint yolo-pro/src/**/*.js tests/**/*.js --fix"
  },
  "keywords": [
    "automation",
    "cli",
    "github",
    "git",
    "swarm",
    "yolo-pro",
    "yolo-warp",
    "milestone",
    "ai"
  ],
  "author": "YOLO-PRO Team",
  "license": "MIT",
  "dependencies": {
    "@octokit/plugin-retry": "^6.0.1",
    "@octokit/plugin-throttling": "^8.1.3",
    "@octokit/rest": "^20.0.2",
    "axios": "^1.11.0",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "yolo-pro/src/**/*.js",
      "!yolo-pro/src/cli/index.js"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
